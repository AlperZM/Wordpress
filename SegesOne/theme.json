{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
        "version": 2,
	"settings": {
		"layout": {
			"contentSize": "840px",
			"wideSize": "1100px"
		},
		"spacing": {
        "padding": true,
	"margin": true,
	"blockGap": true,
	"units": [ "px", "em", "rem", "vh", "vw" ],
},
		"border": {
	"color": true,
	"radius": true,
	"style": true,
	"width": true
},
	"color" : {
		"palette": [
			{
				"slug": "primary",
				"color": "#222831",
				"name": "Primary",
			},
			{
				"slug": "secondary",
				"color": "#393E46",
				"name": "Secondary",
			},
			{
				"slug": "third",
				"color": "#00ADB5",
				"name": "Third",
			},
			{
				"slug": "fourth",
				"color": "#EEEEEE",
				"name": "Fourth",
			},
		],
		"gradients" : [
			"slug": "default-gradient",
			"gradient": "linear-gradient(90deg, rgba(34,40,49,1) 0%, rgba(57,62,70,1) 20%, rgba(0,173,181,1) 100%); ",
			"name": "Default-Gradient",
		],
	},

	"typography": {
		"lineHeight": true,
        "customFontSize": false,
        "dropCap": false,
		"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"slug": "system-fonts",
					"name": "System Fonts"
				},
				{
					"fontFamily": "Geneva, Tahoma, Verdana, sans-serif",
					"slug": "geneva-verdana"
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": "1rem",
					"name": "Small"
				},
				{
					"slug": "extra-large",
					"size": "3rem",
					"name": "Extra large"
				}
			]
	},
		
	"bloks" : {
		"core-heading": {
			"color": {
		"palette": [
			{
				"slug": "tertiary",
				"color": "#1d1f35",
				"name": "Tertiary"
			}
		]
       }
		},
	},


	
	},

	"styles": {
		"color": {
			"background": "var(--wp--preset--color--primary)",
			"text": "var(--wp--preset--color--fourth)"
		}
	},

	"templateParts": [
	{
		"name": "header",
		"area": "header",
		"title": "Header"
	}
]
}
