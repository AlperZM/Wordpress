{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
        "version":2,
	"settings": {
		"layout": {
	"contentSize": "840px",
	"wideSize": "1100px"
},
		
		"spacing": {
	"padding": true,
        "margin": true,
	"units": [ "px", "em", "rem", "vh", "vw", "%" ]
},
		
    "color": {
      "palette": [
        {
				"slug": "primary",
				"color": "#222831",
				"name": "Primary",
			},
			{
				"slug": "secondary",
				"color": "#393E46",
				"name": "Secondary",
			},
			{
				"slug": "third",
				"color": "#00ADB5",
				"name": "Third",
			},
			{
				"slug": "fourth",
				"color": "#EEEEEE",
				"name": "Fourth",
			},
		],
		"gradients" : [
			"slug": "default-gradient",
			"gradient": "linear-gradient(90deg, rgba(34,40,49,1) 0%, rgba(57,62,70,1) 20%, rgba(0,173,181,1) 100%); ",
			"name": "Default-Gradient",
		],
    },
"typography": {
	"fontFamilies": [
		{
			"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
			"slug": "system-fonts",
			"name": "System Fonts"
		},
		{
	                "fontFamily": "Geneva, Tahoma, Verdana, sans-serif",
			"slug": "geneva-verdana"
		},
		{
			"fontFamily": "Cambria, Georgia, serif",
			"slug": "cambria-georgia"
		}
       ],
	"fontSizes": [
	{
		"slug": "extra-small",
		"size": "1rem",
		"name": "Extra small"
	},
	{
		"slug": "small",
		"size": "1.125rem",
		"name": "Small"
	},
	{
		"slug": "medium",
		"size": "1.25rem",
		"name": "Medium"
	},
	{
		"slug": "large",
		"size": "1.5rem",
		"name": "Large"
	},
	{
		"slug": "extra-large",
		"size": "2.5rem",
		"name": "Extra large"
	}
],
	"lineHeight": true,
	
},
		
	},

  "styles": {
	  "color": {
			"background": "var(--wp--preset--color--primary)",
			"text": "var(--wp--preset--color--fourth)",
		},
	  "elements": {
		  "link":{
			  "color": {
				  "text": var(--wp--preset--color--third),
			  },
			  "typography": {
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontFamily": "var(--wp--preset--font-family--system-fonts)",
			"lineHeight": "1.7"
		},
		  },
	  },
  },

  "templateParts": [
	{
		"name": "header",
		"area": "header",
		"title": "Header"
	},
	{
		"name": "footer",
		"area": "footer",
		"title": "Footer"
	}
]
}
